<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="180.0" prefWidth="624.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.fx.inputcontroller.SetScheduleController">
   <children>
      <ComboBox id="instructorComboBox" fx:id="instructorComboBox" layoutX="93.0" layoutY="57.0" onAction="#instructorSelected" prefWidth="150.0" promptText="Select Instructor" />
      <Label layoutX="31.0" layoutY="61.0" text="Instructor" />
      <Button id="mainMenuButton" fx:id="mainMenuButton" layoutX="433.0" layoutY="141.0" mnemonicParsing="false" onAction="#navigateToMainMenu" prefWidth="78.0" text="Main Menu" />
      <Button fx:id="finishButton" layoutX="525.0" layoutY="141.0" mnemonicParsing="false" onAction="#setScheduleAction" prefWidth="78.0" text="Finish" />
      <TextField id="instructorTime" fx:id="instructorTime" layoutX="449.0" layoutY="57.0" prefHeight="25.0" prefWidth="31.0" />
      <Label layoutX="358.0" layoutY="61.0" text="Instructor Time" />
      <Label layoutX="14.0" layoutY="14.0" text="Set Regular Activity Schedule">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="previousButton" layoutX="358.0" layoutY="141.0" mnemonicParsing="false" onAction="#navigateToPrevious" text="Previous" />
   </children>
</AnchorPane>
